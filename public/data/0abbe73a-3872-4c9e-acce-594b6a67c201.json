lex":"0 1 auto","align":null}},"interactions":[]},"content":{"default":{"source":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>ERD Visualization</title>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/cytoscape/3.22.1/cytoscape.min.js\"></script>\n    <style>\n        #cy {\n            width: 100%;\n            height: 800px;\n            border: 1px solid #ccc;\n            font-family: Arial, sans-serif;\n            margin: 20px;\n            background-color: #1c1c1c;\n            color: #ffffff;          \n        }\n\n        .node-table-title {\n            background-color: #444;\n            padding: 5px;\n            border-radius: 3px;\n            text-align: center;\n            font-weight: bold;\n            font-size: 16px;\n            margin-bottom: 10px;\n        }\n\n        .node-table-content {\n            font-size: 12px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <div id=\"cy\"></div>\n\n    <script>\n        const cy = cytoscape({\n            container: document.getElementById('cy'),\n            elements: [\n                // Nodes (Tables)\n                { data: { id: 'Formulario', label: 'Formulario'} },\n                { data: { id: 'Kanban', label: 'Kanban'} },\n                { data: { id: 'bbdd', label: 'Base de datos'} },\n                { data: { id: 'Landing', label: 'Landing'} },\n                { data: { id: 'Export', label: 'Export'} }, \n                { data: { id: 'Notificacion', label: 'Notificación'} }, \n                { data: { id: 'CRM', label: 'CRM'} },               \n\n                // Edges (Relationships)\n                { data: { source: 'Formulario', target: 'bbdd' } },\n                { data: { source: 'Formulario', target: 'Notificacion' } },              \n                { data: { source: 'bbdd', target: 'Kanban' } },\n                { data: { source: 'Landing', target: 'Formulario' } },\n                { data: { source: 'bbdd', target: 'Export' } }, \n                { data: { source: 'bbdd', target: 'CRM' } },               \n\n            ],\n            style: [\n                {\n                    selector: 'node',\n                    style: {\n                        'background-color': '#2c2c2c',\n                        'shape': 'round-rectangle',\n                        'width': '120px',\n                        'height': '40px',\n                        'content': 'data(label)',\n                        'text-valign': 'center',\n                        'text-halign': 'center',\n                        'color': '#fff',\n                        'font-size': '16px',\n                        'padding': '10px',\n                        'border-width': 2,\n                        'border-color': '#555',\n                        'box-shadow': '0px 4px 10px rgba(0, 0, 0, 0.5)'\n                    }\n                },\n                {\n                    selector: 'edge',\n                    style: {\n                        'width': 1,\n                        'line-color': '#fff',\n                        'target-arrow-color': '#888',\n                        'target-arrow-shape': 'triangle',\n                        //'curve-style': 'taxi', // cuadrado\n                        'curve-style': 'unbundled-bezier', // Curvas naturales\n                        'line-style': 'dotted'  // Línea punteada\n                    }\n                },\n                {\n                    selector: 'node:selected',\n                    style: {\n                        'border-width': 7,\n                        'border-color': '#fff'\n                    }\n                }\n            ],\n            layout: {\n                name: 'breadthfirst', \n                padding: 30\n            }\n        });\n\n        cy.nodes().forEach(node => {\n            const label = node.data('label');\n            const description = node.data('description');\n            node.qtip({\n                content: `<div class=\"node-table\">\n                            <div class=\"node-table-title\">${label}</div>\n                            </div>\n                        </div>`,\n                position: {\n                    my: 'top center',\n                    at: 'bottom center'\n                },\n                style: {\n                    classes: 'qtip-dark qtip-rounded',\n                    tip: {\n                        width: 10,\n                        height: 10\n                    }\n                }\n            });\n        });\n    </script>\n</body>\n</html>","refreshOnResize":true}},"name":null,"wwObjectBaseId":"6d692ca2-6cdc-4805-aa0c-211102f335d0","libraryComponentBaseId":null,"parentSectionId":"fc8e3add-8f02-4497-8d3c-c18f49e934a1"}},"collections":[],"variables":[{"id":"d96ef2e9-f428-4f5a-b482-658b17a725ee","type":"boolean","name":"isOpened_copy","defaultValue":true,"isLocalStorage":false,"isPersistentOnNav":true},{"id":"f3b57de5-cf60-4812-8b5e-f9e04a78f470","type":"number","name":"menuWidth_copy","defaultValue":240,"isLocalStorage":false,"isPersistentOnNav":true}],"workflows":[],"formulas":[],"libraryComponents":[]}